<script>
function confirm_deletion(ttl){
	var msg = "Сигурни ли сте, че искате да изтриете този текст?"
	return confirm(msg);
}

function showInfo(obj, ids){
	var content = "<div>";
	$(ids).each
	(
		function (idx, id){
			obj_class = $(id).attr("class");
			if (obj_class == "comment"){
				title = "Бележка под линия";
			}
			else{
				title = "Разночетене";
			}
			content = content+"<h1>"+title+"<\/h1>";
			content = content +"<p>"+$(id).html()+"<\/p>";
		}
	);
	content = content+"<\/div>";
	$(obj).tipTip({content: content, maxWidth: "400px", defaultPosition: "top", delay: 100, activation: "click", keepAlive: true});
}

$(window).load
(
	function (){
		if ($("div.editor_text").html().trim() == ""){
			$("div.editor_text").css("margin-top", "0px");
		}
	}
);

</script>


